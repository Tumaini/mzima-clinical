<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mcddb.data.ClinicalMain" table="clinical_main" schema="public" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="date" type="date">
            <column name="date" length="13"/>
        </property>
        <property name="serolinknumber" type="string">
            <column name="serolinknumber"/>
        </property>
        <property name="age" type="integer">
            <column name="age"/>
        </property>
        <property name="gender" type="string">
            <column name="gender" length="1"/>
        </property>
        <property name="bmi" type="double">
            <column name="bmi" precision="17"/>
        </property>
        <property name="clinicalcomplaint" type="string">
            <column name="clinicalcomplaint"/>
        </property>
        <property name="pregnant" type="string">
            <column name="pregnant"/>
        </property>
        <property name="insertby" type="integer">
            <column name="insertby"/>
        </property>
        <property name="insertdate" type="date">
            <column name="insertdate" length="13"/>
        </property>
        <property name="clinician" type="string">
            <column name="clinician"/>
        </property>
    </class>
</hibernate-mapping>