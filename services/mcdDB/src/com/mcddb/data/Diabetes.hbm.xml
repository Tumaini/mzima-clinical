<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mcddb.data.Diabetes" table="diabetes" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="wanttoknowresult" type="string">
            <column name="wanttoknowresult" length="20"/>
        </property>
        <property name="bloodsugarlevel" type="double">
            <column name="bloodsugarlevel" precision="22"/>
        </property>
        <property name="knowndiabetic" type="string">
            <column name="knowndiabetic" length="20"/>
        </property>
        <property name="ontreatment" type="string">
            <column name="ontreatment" length="20"/>
        </property>
        <property name="symptoms" type="string">
            <column name="symptoms" length="20"/>
        </property>
        <property name="physicalexam" type="string">
            <column name="physicalexam" length="20"/>
        </property>
        <property name="referred" type="string">
            <column name="referred" length="20"/>
        </property>
        <property name="serolinknumber" type="string">
            <column name="serolinknumber" length="20"/>
        </property>
        <property name="insertby" type="integer">
            <column name="insertby"/>
        </property>
        <property name="insertdate" type="date">
            <column name="insertdate" length="10"/>
        </property>
        <property name="clinician" type="string">
            <column name="clinician" length="20"/>
        </property>
    </class>
</hibernate-mapping>