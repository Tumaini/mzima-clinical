<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mcddb.data.Tb" table="tb" schema="public" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="knowntbpatient" type="string">
            <column name="knowntbpatient"/>
        </property>
        <property name="ontbtreatment" type="string">
            <column name="ontbtreatment"/>
        </property>
        <property name="treatedlast12months" type="string">
            <column name="treatedlast12months"/>
        </property>
        <property name="symptoms" type="string">
            <column name="symptoms"/>
        </property>
        <property name="physicalexam" type="string">
            <column name="physicalexam"/>
        </property>
        <property name="participanttobereferred" type="string">
            <column name="participanttobereferred"/>
        </property>
        <property name="serolinknumber" type="string">
            <column name="serolinknumber"/>
        </property>
        <property name="insertby" type="integer">
            <column name="insertby"/>
        </property>
        <property name="insertdate" type="date">
            <column name="insertdate" length="13"/>
        </property>
        <property name="clinician" type="string">
            <column name="clinician"/>
        </property>
    </class>
</hibernate-mapping>