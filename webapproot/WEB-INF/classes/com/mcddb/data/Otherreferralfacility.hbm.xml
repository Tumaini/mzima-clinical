<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mcddb.data.Otherreferralfacility" table="otherreferralfacility" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="serolinknumber" type="string">
            <column name="serolinknumber" length="20"/>
        </property>
        <property name="facility" type="string">
            <column name="facility" length="50"/>
        </property>
        <property name="insertby" type="string">
            <column name="insertby" length="20"/>
        </property>
        <property name="insertdate" type="date">
            <column name="insertdate" length="10"/>
        </property>
        <property name="clinician" type="string">
            <column name="clinician" length="20"/>
        </property>
    </class>
</hibernate-mapping>